{
  "expo": {
    "name": "crewhq",
    "slug": "crewhq",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "crewhq",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ashleyglenn0.crewhq"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.ashleyglenn0.crewhq"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications"
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "599315aa-e22e-4ee0-9f8a-a075e991797d"
      },
      "QR_BYPASS": "true"
    },
    "owner": "ashleyglenn0"
  }
}
